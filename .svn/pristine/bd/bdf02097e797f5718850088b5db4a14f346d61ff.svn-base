/* BOOSTRAP OVERVISE */
.btn, .panel-default, .form-control,.list-group-item {
    border-radius: 0!important;
}
textarea.form-control{
    resize: none;
}
.form-group.required label:after {
    content: ' *';
}

.form-control,.bootstrap-tagsinput{
    background-color: rgba(255,255,255,0);
    transition: ease-in-out,.2s,all;
    border: none;
    border-bottom: 1px solid #CCC;
    box-shadow: none!important;
    padding: 0;
}
.form-control[contenteditable=true]{
    height: auto;
    min-height: 34px;
}
.form-control:hover,.bootstrap-tagsinput:hover{
    border-bottom: 1px solid #5BC0DE;
}
.form-control:focus,.bootstrap-tagsinput:active{
    border-bottom: 2px solid #5BC0DE;
}
.panel-default > .panel-heading{
    background: initial;
    font-size: 16px;
    border: none;
    box-shadow: 0px 3px 5px -2px #DDD;
}
.list-group,.panel{
    box-shadow: 0 6px 5px -4px #ccc;
}
.panel {
    background-color: #FFF;
}
.page{
    margin-top: 60px;
}
@media (max-width: 992px) {

}
#event-search{
    width: 85%;
}

@media (max-width: 768px) {
    .navbar-inverse .navbar-nav .open .dropdown-menu > li > a{
        color: #EEE;
    }
}
@media (max-width: 992px) {
    #event-search{
        width: 100%;
    }
}
@media (min-width: 768px) {
    .page{
        margin-top: inherit;
    }
    #user-main-link .username,#user-main-link .caret{
        display: none!important;
    }
    .navbar {
        position: relative;
        min-height: 30px;
        margin-bottom: 5px;
        border: none;
    }

    li.user-main-avatar {
        width: 50px;
        margin-right: 15px;
        margin-left: 15px;
    }
    .user-main-avatar .ava{
        z-index: 9999;
    }
    #user-main-link{
        height: 31px;
        color: inherit;
        background-color: inherit;
    }

    li.user-main-avatar .ava {
        transition: ease-out,.3s,all;
        box-shadow: 0 0 0 2px #5290C5;
        height: 50px;
        width: 50px;
        line-height: 50px;
        font-size: 25px;
        position: absolute;
        left: 0;
        top: 5px;
    }
    li.user-main-avatar .ava:hover {
        box-shadow: 0 0 0 4px #5290C5;
    }
    .navbar-inverse .navbar-nav > li:not(.user-main-avatar) > a:hover,
    .navbar-inverse .navbar-nav > li:not(.user-main-avatar) > a:focus {
        border-color: #FFF;
        background-color: inherit;
    }

    .navbar-inverse .navbar-nav > li:not(.user-main-avatar) > a,
    .navbar-inverse .navbar-nav > li:not(.user-main-avatar) > a {
        -webkit-transition: border-color .300s;
        transition: border-color .300s;
        background-color: inherit;
        border-left: none;
        border-right: none;
        border-top: none;
        border-bottom: 2px solid transparent;
        padding: 0;
        margin: 0 15px;
    }

    .navbar-inverse .navbar-nav > li.active:not(.user-main-avatar) > a,
    .navbar-inverse .navbar-nav > li.active:not(.user-main-avatar) > a {

        color: #fff;
        background-color: inherit;
        border-color: #FFF;
    }

    .navbar-brand {
        float: left;
        height: 32px;
        padding: 0 15px;
        font-size: 18px;
        line-height: 32px;
    }

    .navbar-nav > li > a {
        padding-top: inherit;
        padding-bottom: inherit;
        line-height: 30px

    }

    .main-form {
        margin-top: 75px;
    }
}

.media.events {
    border-bottom: 1px solid #f2f2f2;
    padding-bottom: 10px;
}
.bootstrap-tagsinput{
    display: block;
    width: 100%;
    height: inherit;
    padding: 6px 0;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    border-radius: 0;
    -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.event-next-page{
    cursor: pointer;
    margin-top: -30px;
    color: #666;
    box-shadow: 0px 7px 7px -4px #999;
    transition: all ease .3s;
    font-size: 60px;
    height: 60px;
    line-height: 60px;
    width: 60px;
    border-radius: 50px;
    background: #F1F1F1;
}
.event-next-page:hover{
    color: #666;
    box-shadow: 0px 7px 7px -4px #5E5E5E;
}

.panel-next-page{
    text-align: center; font-size: 28px; cursor: pointer; color: #c7c7c7;
}
.event-img{
    position: relative;
}
.event-img:hover .event-control{
    background-color: rgba(255,255,255,0.5);
    color:#666;
}
.event-control{
    position: absolute;
    transition: ease, .2s, all;
    text-align: right;
    padding: 0px 6px;
    width: 100%;
    font-size: 25px!important;
    color: #404040;
    opacity: 0.9;
    top: -2px;
    right: 0;
}
.panel-body > .media{
    border: 0;
    border-top: 1px solid #eee;
}
.panel-body > .media > .media-body{
    padding-top: 5px;
}
.panel-body > .media:first-child{
    border-top: inherit;
}
.panel-body > .media:first-child > .media-body{
    padding-top: inherit;
}
.event-control .md,.event-control-line .md{
    transition: ease, 0.4s,all;
    cursor: pointer;
}
.event-control-line {
    transition: ease-out,.3s,opacity;
    float: right;
    font-size: 20px;
    opacity: 0.5;
}
.event-control-line:hover {
    opacity: 1;
}
.event-control-subscribe:hover{
    color: #318038;
}
.event-control-subscribe.md-add-circle {
    color: #318038;
}
.event-control-view{
    color: #404040;
}
.event-control-view:hover{
    color: inherit;
}
.user-point-on{
    color: #55A656;
    text-shadow: 0 0 4px #468847;
}
.user-point-off{
    color: #999;
}

/* EVENT CALENDAR */

.event-calendar {
    list-style: none;
    margin: 0px 0px 5px 0px;
    padding: 0px;
    overflow: hidden;
}
.event-calendar > li {
    transition: ease-out,.3s,all;
    float: left;
    width: 30px;
    height: 40px;
    text-align: center;
    border-top: 3px solid;
    border-color: #989898;
    line-height: 14px;
    padding-top: 5px;
}
.event-calendar > li.active:not(.disabled),.event-calendar > .event-calendar-all.active:not(.disabled) {
    background-color: #2786DF;
    color: #F1F1F1;
}
.event-calendar > li.disabled > a{
    color: #CCC;
    cursor: default;
}
.event-calendar > li.active > a {
    color: #F1F1F1;
}
.event-calendar > li.event-calendar-all {
    width: auto;
    line-height: 32px;
    cursor: pointer;
    padding-top: 2px;
    font-size: 30px;
    color: #999;
}
.event-calendar > li:not(.disabled):hover,.event-calendar > .event-calendar-all:not(.disabled):hover {
    background: rgb(255, 255, 255);
}
.event-calendar > li:not(.disabled).active:hover > a,.event-calendar > .event-calendar-all:not(.disabled):hover{
    color: #595959;
}
.event-calendar > li.weekend {
    border-color: rgb(208, 5, 5);
}

.event-calendar > li > a {
    text-decoration: none;
    color: #595959;
    font-family: "PT Sans","Helvetica Neue",Helvetica,Arial,sans-serif;
}
.event-calendar > li > a > span {
    font-weight: bold;
}
.event-calendar > li > a > small {
    display: block;
}
.user-view-counter{
